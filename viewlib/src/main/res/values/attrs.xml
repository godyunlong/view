<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LabelView">
        <attr format="dimension" name="lab_padding_left"/>
        <attr format="dimension" name="lab_padding_right"/>
        <attr format="dimension" name="lab_padding_top"/>
        <attr format="dimension" name="lab_padding_bottom"/>
        <attr format="dimension" name="lab_padding_vertical"/>
        <attr format="dimension" name="lab_padding_horizontal"/>

        <attr format="dimension" name="lab_padding"/>
        <attr format="dimension" name="lab_space_vertical"/>
        <attr format="dimension" name="lab_space_horizontal"/>
        <attr format="dimension" name="lab_text_size"/>
        <attr format="reference" name="lab_text_list_color"/>
        <attr format="color" name="lab_text_common_color"/>
        <attr format="reference" name="lab_background"/>
        <attr format="dimension" name="lab_mine_size"/>
    </declare-styleable>

    <declare-styleable name="ImageMoreView">
        <attr format="dimension" name="image_more_space_vertical"/>
        <attr format="dimension" name="image_more_space_horizontal"/>
        <attr format="dimension" name="image_more_space"/>
        <attr format="dimension" name="image_more_radius"/>
        <attr format="dimension" name="image_more_item_height"/>
        <attr format="integer" name="image_more_cow"/>
    </declare-styleable>

    <declare-styleable name="MoneyEditView">
        <attr name="money_label_text" format="string" />
        <attr name="money_label_color" format="color"/>
        <attr name="money_label_size" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="NavigationView">
        <attr name="aj_navigation_can_scroll" format="color"/>
        <attr name="aj_navigation_select_background" format="reference"/>
        <attr name="aj_navigation_select_not_background" format="reference"/>
        <attr name="aj_navigation_speed" format="integer"/>
    </declare-styleable>

    <declare-styleable name="DateView">
        <attr name="date_select_background" format="reference"/>
        <attr name="date_select_textColor" format="color"/>
        <attr name="date_select_background_not" format="reference"/>
        <attr name="date_select_textColor_not" format="color"/>
        <attr name="date_toDay_background" format="reference"/>
        <attr name="date_toDay_textColor" format="color"/>
        <attr name="date_common_textColor" format="color"/>
    </declare-styleable>


    <declare-styleable name="QQMessageView">
        <attr name="fixCircleRadiusMax" format="dimension" />
        <attr name="fixCircleRadiusMin" format="dimension" />
        <attr name="dragCircleRadius" format="dimension" />
        <attr name="circleColor" format="color" />
    </declare-styleable>
</resources>